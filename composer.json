{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "5.0.*",
        "caseyamcl/Phpoaipmh": "dev-master",
        "intervention/image": "dev-master",
        "guzzlehttp/guzzle": "dev-master",
        "fadion/bouncy": "dev-l5",
        "rtconner/laravel-tagging": "~2.1",
        "maatwebsite/excel": "~2.1.0",
        "rap2hpoutre/laravel-log-viewer": "dev-master",
        "zizaco/entrust": "1.4.1",
        "roumen/sitemap": "dev-master",
        "cviebrock/image-validator": "dev-master",
        "anahkiasen/former": "^4.0",
        "laravelcollective/html": "~5.0",
        "barryvdh/laravel-debugbar": "~2.1.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["app/Helpers/general.php"]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}